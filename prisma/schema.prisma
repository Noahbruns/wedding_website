generator client {
    provider = "prisma-client-js"
    output   = "../generated/prisma"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model LeaderboardEntry {
    id        Int      @id @default(autoincrement())
    name      String   @db.VarChar(50)
    score     Int
    createdAt DateTime @default(now())
}
